# Neo4j Connection Configuration
# Copy this file to .env and update with your actual credentials

# Neo4j URI (Bolt protocol)
NEO4J_URI=bolt://localhost:7687

# Neo4j Authentication
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=your-password-here

# Neo4j Database (default is "neo4j")
NEO4J_DATABASE=neo4j

# Optional: Path to custom n10s config
# NEO4J_CONFIG_FILE=scripts/neo4j_import_config.json

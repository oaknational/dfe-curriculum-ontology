@prefix curric: <https://w3id.org/uk/curriculum/core/> .
@prefix eng: <https://w3id.org/uk/curriculum/england/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ======================================================================
# National Curriculum for England - Programme Structure
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/england/programme-structure>
  rdf:type owl:Ontology ;

  rdfs:label "National Curriculum for England - Programme Structure"@en ;
  dc:title "National Curriculum for England - Programme Structure"@en ;
  rdfs:comment """Programme structure defining the organizational framework of education 
  in England: phases, key stages, and year groups."""@en ;

  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/england/programme-structure/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/england/programme-structure/0.0.9> ;
  
  dcterms:description """This ontology defines the organizational structure of education in 
  England, including phases (Primary, Secondary), key stages (KS1-4), and year groups (Year 1 - 
  Year 11) that provide the framework for curriculum delivery."""@en ;
  dc:creator "Department for Education" ;
  dcterms:created "2025-11-24"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;
  
  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> .

# ----------------------------------------------------------------------
# Phases
# ----------------------------------------------------------------------

eng:phase-primary
  a curric:Phase ;
  rdfs:label "Primary"@en ;
  rdfs:comment "Primary education phase covering years 1-6."@en ;
  curric:lowerAgeBoundary "5"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "11"^^xsd:positiveInteger .

eng:phase-secondary
  a curric:Phase ;
  rdfs:label "Secondary"@en ;
  rdfs:comment "Secondary education phase covering years 7-11."@en ;
  curric:lowerAgeBoundary "11"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "16"^^xsd:positiveInteger .

eng:education-phases
  a skos:OrderedCollection ;
  rdfs:label "Education Phases"@en ;
  skos:memberList (eng:phase-primary eng:phase-secondary) .

# ----------------------------------------------------------------------
# Key Stages
# ----------------------------------------------------------------------

eng:key-stage-1
  a curric:KeyStage ;
  rdfs:label "Key Stage 1"@en ;
  rdfs:comment "Key Stage 1 covering years 1-2."@en ;
  curric:lowerAgeBoundary "5"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "7"^^xsd:positiveInteger ;
  curric:isKeyStageOf eng:phase-primary .

eng:key-stage-2
  a curric:KeyStage ;
  rdfs:label "Key Stage 2"@en ;
  rdfs:comment "Key Stage 2 covering years 3-6."@en ;
  curric:lowerAgeBoundary "7"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "11"^^xsd:positiveInteger ;
  curric:isKeyStageOf eng:phase-primary .

eng:key-stage-3
  a curric:KeyStage ;
  rdfs:label "Key Stage 3"@en ;
  rdfs:comment "Key Stage 3 covering years 7-9."@en ;
  curric:lowerAgeBoundary "11"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "14"^^xsd:positiveInteger ;
  curric:isKeyStageOf eng:phase-secondary .

eng:key-stage-4
  a curric:KeyStage ;
  rdfs:label "Key Stage 4"@en ;
  rdfs:comment "Key Stage 4 covering years 10-11 (GCSE years)."@en ;
  curric:lowerAgeBoundary "14"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "16"^^xsd:positiveInteger ;
  curric:isKeyStageOf eng:phase-secondary .

eng:primary-key-stages
  a skos:OrderedCollection ;
  rdfs:label "Primary Key Stages"@en ;
  skos:memberList (eng:key-stage-1 eng:key-stage-2) .

eng:secondary-key-stages
  a skos:OrderedCollection ;
  rdfs:label "Secondary Key Stages"@en ;
  skos:memberList (eng:key-stage-3 eng:key-stage-4) .

# ----------------------------------------------------------------------
# Year Groups
# ----------------------------------------------------------------------

eng:year-group-1
  a curric:YearGroup ;
  rdfs:label "Year Group 1"@en ;
  rdfs:comment "Year Group 1 (age 5-6)."@en ;
  curric:lowerAgeBoundary "5"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "6"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-1 .

eng:year-group-2
  a curric:YearGroup ;
  rdfs:label "Year Group 2"@en ;
  rdfs:comment "Year Group 2 (age 6-7)."@en ;
  curric:lowerAgeBoundary "6"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "7"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-1 .

eng:year-group-3
  a curric:YearGroup ;
  rdfs:label "Year Group 3"@en ;
  rdfs:comment "Year Group 3 (age 7-8)."@en ;
  curric:lowerAgeBoundary "7"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "8"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-2 .

eng:year-group-4
  a curric:YearGroup ;
  rdfs:label "Year Group 4"@en ;
  rdfs:comment "Year Group 4 (age 8-9)."@en ;
  curric:lowerAgeBoundary "8"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "9"^^xsd:positiveInteger ;

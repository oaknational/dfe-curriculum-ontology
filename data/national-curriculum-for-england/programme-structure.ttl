eng:key-stage-1-year-groups
  a skos:OrderedCollection ;
  rdfs:label "Key Stage 1 Year Groups"@en ;
  skos:memberList (eng:year-group-1 eng:year-group-2) .

eng:key-stage-2-year-groups
  a skos:OrderedCollection ;
  rdfs:label "Key Stage 2 Year Groups"@en ;
  skos:memberList (eng:year-group-3 eng:year-group-4 eng:year-group-5 eng:year-group-6) .

eng:key-stage-3-year-groups
  a skos:OrderedCollection ;
  rdfs:label "Key Stage 3 Year Groups"@en ;
  skos:memberList (eng:year-group-7 eng:year-group-8 eng:year-group-9) .

eng:key-stage-4-year-groups
  a skos:OrderedCollection ;
  rdfs:label "Key Stage 4 Year Groups"@en ;
  skos:memberList (eng:year-group-10 eng:year-group-11) .
  
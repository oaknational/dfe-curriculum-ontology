eng:year-group-5
  a curric:YearGroup ;
  rdfs:label "Year Group 5"@en ;
  rdfs:comment "Year Group 5 (age 9-10)."@en ;
  curric:lowerAgeBoundary "9"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "10"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-2 .

eng:year-group-6
  a curric:YearGroup ;
  rdfs:label "Year Group 6"@en ;
  rdfs:comment "Year Group 6 (age 10-11)."@en ;
  curric:lowerAgeBoundary "10"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "11"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-2 .

eng:year-group-7
  a curric:YearGroup ;
  rdfs:label "Year Group 7"@en ;
  rdfs:comment "Year Group 7 (age 11-12)."@en ;
  curric:lowerAgeBoundary "11"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "12"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-3 .

eng:year-group-8
  a curric:YearGroup ;
  rdfs:label "Year Group 8"@en ;
  rdfs:comment "Year Group 8 (age 12-13)."@en ;
  curric:lowerAgeBoundary "12"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "13"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-3 .

eng:year-group-9
  a curric:YearGroup ;
  rdfs:label "Year Group 9"@en ;
  rdfs:comment "Year Group 9 (age 13-14)."@en ;
  curric:lowerAgeBoundary "13"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "14"^^xsd:positiveInteger ;
  curric:isYearGroupOf eng:key-stage-3 .

eng:year-group-10
  a curric:YearGroup ;
  rdfs:label "Year Group 10"@en ;
  rdfs:comment "Year Group 10 (age 14-15), first year of GCSE study."@en ;
  curric:lowerAgeBoundary "14"^^xsd:nonNegativeInteger ;

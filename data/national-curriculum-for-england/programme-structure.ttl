@prefix curric: <https://w3id.org/uk/curriculum/core/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix eng: <https://w3id.org/uk/curriculum/england/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

eng:key-stage-3 a curric:KeyStage ;
    rdfs:label "Key Stage 3"@en ;
    rdfs:comment "Key Stage 3 covering years 7-9."@en ;
    curric:isPartOf eng:phase-secondary ;
    curric:lowerAgeBoundary "11"^^xsd:nonNegativeInteger ;
    curric:upperAgeBoundary "14"^^xsd:positiveInteger .

eng:key-stage-4 a curric:KeyStage ;
    rdfs:label "Key Stage 4"@en ;
    rdfs:comment "Key Stage 4 covering years 10-11."@en ;
    curric:isPartOf eng:phase-secondary ;
    curric:lowerAgeBoundary "14"^^xsd:nonNegativeInteger ;
    curric:upperAgeBoundary "16"^^xsd:positiveInteger .

eng:phase-primary a curric:Phase ;
    rdfs:label "Primary"@en ;
    rdfs:comment "Primary education phase covering years 1-6."@en ;
    curric:lowerAgeBoundary "5"^^xsd:nonNegativeInteger ;
    curric:upperAgeBoundary "11"^^xsd:positiveInteger .

eng:programme-structure a owl:Ontology ;
    rdfs:label "National Curriculum for England - Programme Structure"@en ;
    dc:title "National Curriculum for England - Programme Structure"@en ;
    dcterms:created "2025-12-20"^^xsd:date ;
    dcterms:creator "Department for Education" ;
    dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
    dcterms:rights "Crown Copyright"@en ;
    rdfs:comment "Programme structure defining phases, key stages, and year groups."@en ;
    owl:imports curric: ;
    owl:versionInfo "0.1.0" .

eng:phase-secondary a curric:Phase ;
    rdfs:label "Secondary"@en ;
    rdfs:comment "Secondary education phase covering years 7-11."@en ;
    curric:lowerAgeBoundary "11"^^xsd:nonNegativeInteger ;
    curric:upperAgeBoundary "16"^^xsd:positiveInteger .


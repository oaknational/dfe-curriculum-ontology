eng:year-group-11
  a curric:YearGroup ;
  rdfs:label "Year Group 11"@en ;
  rdfs:comment "Year Group 11 (age 15-16), final year of GCSE study."@en ;
  curric:lowerAgeBoundary "15"^^xsd:nonNegativeInteger ;
  curric:upperAgeBoundary "16"^^xsd:positiveInteger ;

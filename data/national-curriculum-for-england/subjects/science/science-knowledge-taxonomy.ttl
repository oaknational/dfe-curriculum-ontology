eng:content-descriptor-observe-seasonal-changes a skos:Concept,
        curric:ContentDescriptor ;
    skos:broader eng:substrand-seasonal-changes ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Observe changes across the 4 seasons"@en .

eng:strand-material-cycles-and-energy a skos:Concept,
        curric:Strand ;
    skos:broader eng:discipline-science ;
    skos:definition "Study of how materials and energy flow through natural systems."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Material cycles and energy"@en .

eng:substrand-seasonal-changes a skos:Concept,
        curric:SubStrand ;
    skos:broader eng:strand-material-cycles-and-energy ;
    skos:definition "Patterns and changes that occur across different seasons."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Seasonal changes"@en .

eng:discipline-science a skos:Concept,
        curric:Discipline ;
    skos:definition "The systematic study of the natural world through observation, experimentation, and theory, encompassing biology, chemistry, and physics."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Science"@en ;
    skos:scopeNote "Science is essential for understanding the natural world and making informed decisions about scientific developments. It develops critical thinking, analytical skills, and systematic investigation methods that students will use throughout their lives."@en ;
    skos:topConceptOf eng:knowledge-taxonomy .

eng:strand-structure-function-living-organisms a skos:Concept,
        curric:Strand ;
    skos:broader eng:discipline-science ;
    skos:definition "Understanding the organization and functioning of living organisms from cells to systems."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Structure and function of living organisms"@en .

eng:substrand-cells-organisation a skos:Concept,
        curric:SubStrand ;
    skos:broader eng:strand-structure-function-living-organisms ;
    skos:definition "The structure and function of cells as the fundamental units of life."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Cells and organisation"@en .

eng:substrand-skeletal-and-muscular-systems a skos:Concept,
        curric:SubStrand ;
    skos:broader eng:strand-structure-function-living-organisms ;
    skos:definition "How the skeletal and muscular systems work together for movement and support."@en ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "The skeletal and muscular systems"@en .


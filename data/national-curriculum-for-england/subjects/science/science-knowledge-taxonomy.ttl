eng:content-descriptor-biomechanics a skos:Concept,
        curric:ContentDescriptor ;
    skos:broader eng:substrand-skeletal-and-muscular-systems ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Biomechanics"@en .

eng:content-descriptor-cells-as-unit-of-living-organism a skos:Concept,
        curric:ContentDescriptor ;
    skos:broader eng:substrand-cells-organisation ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Cells as the fundamental unit of living organisms, including how to observe, interpret and record cell structure using a light microscope."@en .

eng:content-descriptor-function-of-muscles a skos:Concept,
        curric:ContentDescriptor ;
    skos:broader eng:substrand-skeletal-and-muscular-systems ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "The function of muscles and examples of antagonistic muscles."@en .

eng:content-descriptor-functions-of-the-cell-wall a skos:Concept,
        curric:ContentDescriptor ;
    skos:broader eng:substrand-cells-organisation ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "The functions of the cell wall, cell membrane, cytoplasm, nucleus, vacuole, mitochondria and chloroplasts."@en .

eng:content-subdescriptor-seasons-detail a skos:Concept,
        curric:ContentSubDescriptor ;
    skos:broader eng:content-descriptor-observe-seasonal-changes ;
    skos:inScheme eng:knowledge-taxonomy ;
    skos:prefLabel "Observe and describe weather associated with the seasons and how day length varies"@en ;
    curric:example "Students should observe how trees change through autumn and spring, noting leaf color changes and bud formation."@en .


# ======================================================================
# National Curriculum for England - Science Subject
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/england/science-subject>
  rdf:type owl:Ontology ;

  rdfs:label "National Curriculum for England - Science Subject"@en ;
  dc:title "National Curriculum for England - Science Subject"@en ;
  rdfs:comment """Science subject definition, including aims and strands for the 
  National Curriculum in England."""@en ;

  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/england/science-subject/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/england/science-subject/0.0.9> ;
  
  dcterms:description """This file defines the Science subject in the National Curriculum 
  for England, including its aims and strands within the knowledge taxonomy."""@en ;
  dc:creator "Department for Education" ;
  dcterms:created "2025-11-24"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;
  
  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> .

# ----------------------------------------------------------------------
# Subject: Science
# ----------------------------------------------------------------------

eng:subject-science
  a curric:Subject ;
  rdfs:label "Science"@en ;
  rdfs:comment "Science subject in the National Curriculum for England."@en ;
  curric:coversDiscipline eng:discipline-science .

# ----------------------------------------------------------------------
# Sub-Subjects 
# ----------------------------------------------------------------------

eng:subsubject-science
  a curric:SubSubject ;
  rdfs:label "Science"@en ;
  rdfs:comment "Science sub-subject in the National Curriculum for England."@en ;
  dcterms:description """A high-quality science education provides the foundations for 
  understanding the world through the specific disciplines of biology, chemistry and 
  physics."""@en ;
  dcterms:source <https://www.gov.uk/government/publications/national-curriculum-in-england-science-programmes-of-study> ;
  curric:isSubSubjectOf eng:subject-science ;
  curric:coversStrand eng:strand-structure-function-living-organisms ,
                   eng:strand-material-cycles-and-energy ;

  curric:hasAim 
    """Develop scientific knowledge and conceptual understanding through the specific disciplines 
    of biology, chemistry and physics"""@en ,
    """Develop understanding of the nature, processes and methods of science through different 
    types of science enquiries that help them to answer scientific questions about the world around 
    them"""@en ,
    """Are equipped with the scientific knowledge required to understand the uses and implications 
    of science, today and for the future"""@en .

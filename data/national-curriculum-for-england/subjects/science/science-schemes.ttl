# ======================================================================
# National Curriculum for England - Science Schemes
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/england/science-schemes>
  rdf:type owl:Ontology ;

  rdfs:label "National Curriculum for England - Science Schemes"@en ;
  dc:title "National Curriculum for England - Science Schemes"@en ;
  rdfs:comment """Science schemes that map content descriptors to each scheme."""@en ;

  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/england/science-schemes/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/england/science-schemes/0.0.9> ;
  
  dcterms:description """This file defines Science schemes across all key stages (KS1-4), 
  specifying which content descriptors are taught at each stage."""@en ;
  dc:creator "Department for Education" ;
  dcterms:created "2025-11-24"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;
  
  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> ;
  owl:imports <https://w3id.org/uk/curriculum/england/programme-structure> ;
  owl:imports <https://w3id.org/uk/curriculum/england/science-subject> ;
  owl:imports <https://w3id.org/uk/curriculum/england/science-knowledge-taxonomy> .

# ----------------------------------------------------------------------
# Schemes
# ----------------------------------------------------------------------

eng:scheme-science-key-stage-3
  a curric:Scheme ;
  rdfs:label "Science Key Stage 3"@en ;
  rdfs:comment "Science programme of study for key stage 3."@en ;
  curric:isPartOf eng:subsubject-science ;
  curric:hasKeyStage eng:key-stage-3 ;
  curric:hasContentDescriptor
    eng:content-descriptor-cells-as-unit-of-living-organism ,
    eng:content-descriptor-functions-of-the-cell-wall .

eng:scheme-combined-science-key-stage-4
  a curric:Scheme ;
  rdfs:label "Combined Science Key Stage 4"@en ;
  rdfs:comment "Combined science programme of study for key stage 4."@en ;
  curric:isPartOf eng:subsubject-science ;
  curric:hasKeyStage eng:key-stage-4 ;
  curric:hasContentDescriptor
    eng:content-descriptor-cells-as-unit-of-living-organism ,
    eng:content-descriptor-functions-of-the-cell-wall .

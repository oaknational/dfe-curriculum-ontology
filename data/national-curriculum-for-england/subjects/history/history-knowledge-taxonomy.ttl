  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> ;
  owl:imports <https://w3id.org/uk/curriculum/england/history-subject> .

# ----------------------------------------------------------------------
# Knowledge Taxonomy
# ----------------------------------------------------------------------

eng:knowledge-taxonomy
  a skos:ConceptScheme ;
  skos:prefLabel "National Curriculum for England Knowledge Structure"@en ;
  skos:hasTopConcept eng:discipline-history .
  
# ----------------------------------------------------------------------
# Disciplines
# ----------------------------------------------------------------------

eng:discipline-history
  a skos:Concept, curric:Discipline ;
  skos:prefLabel "History"@en ;
  skos:topConceptOf eng:knowledge-taxonomy ;
  skos:definition """The national curriculum for history develops knowledge and understanding of 
  Britain’s past and that of the wider world."""@en ;
  skos:scopeNote """History helps pupils to understand the complexity of people’s lives, the 
  process of change, the diversity of societies and relationships between different groups, as well 
  as their own identity and the challenges of their time. It equips pupils to ask perceptive 
  questions, think critically, weigh evidence, sift arguments, and develop perspective and 
  judgement."""@en ;
  skos:narrower eng:strand-knowledge-of-past ,
                eng:strand-knowledge-of-history ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Strands
# ----------------------------------------------------------------------

eng:strand-knowledge-of-past
  a skos:Concept, curric:Strand ;
  skos:prefLabel "Knowledge of the past"@en ;
  skos:broader eng:discipline-history ;
  skos:narrower eng:substrand-substantive-concepts ,
                eng:substrand-history-of-diverse-britain ,
                eng:substrand-significant-aspects-of-world-history ;
  skos:inScheme eng:knowledge-taxonomy .

eng:strand-knowledge-of-history
  a skos:Concept, curric:Strand ;
  skos:prefLabel "Knowledge of history as a discipline"@en ;
  skos:broader eng:discipline-history ;
  skos:narrower eng:substrand-second-order-concepts ,
                eng:substrand-nature-of-historical-enquiry ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Sub-Strands
# ----------------------------------------------------------------------

eng:substrand-second-order-concepts
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "Second order concepts"@en ;
  skos:broader eng:strand-knowledge-of-history ;
  skos:narrower eng:content-descriptor-narratives-analyses-accounts ,
                eng:content-descriptor-narratives-analyses-accounts-consequence ,
                eng:content-descriptor-narratives-analyses-accounts-change-and-continuity,
                eng:content-descriptor-narratives-analyses-accounts-similarity-and-difference ,
                eng:content-descriptor-narratives-analyses-accounts-historical-significance ,
                eng:content-descriptor-narratives-analyses-accounts-sources-and-evidence,
                eng:content-descriptor-narratives-analyses-accounts-historical-interpretations ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-nature-of-historical-enquiry
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "The nature of historical enquiry"@en ;
  skos:broader eng:strand-knowledge-of-history ;
  skos:narrower eng:content-descriptor-historians-investigate-the-past ,
                eng:content-descriptor-historians-construct-arguments ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-substantive-concepts
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "Substantive concepts"@en ;
  skos:broader eng:strand-knowledge-of-past ;
  skos:narrower eng:content-descriptor-abstract-terms-historically-grounded ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-history-of-diverse-britain
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "The history of a diverse Britain and its connections to the wider world"@en ;
  skos:broader eng:strand-knowledge-of-past ;
  skos:narrower eng:content-descriptor-changes-within-living-memory ,
                eng:content-descriptor-events-beyond-living-memory ,
                eng:content-descriptor-lives-of-significant-individuals ,
                eng:content-descriptor-significant-historical-events-own-locality ,
                eng:content-descriptor-changes-in-britain-stone-age-to-iron-age ,
                eng:content-descriptor-roman-empire-impact-on-britain ,
                eng:content-descriptor-settlement-by-anglo-saxons-and-scots ,
                eng:content-descriptor-viking-and-anglo-saxon-struggle ,
                eng:content-descriptor-local-history-study ,
                eng:content-descriptor-aspect-british-history-beyond-1066 ,
                eng:content-descriptor-church-state-society-1066-1509 ,
                eng:content-descriptor-church-state-society-1509-1745 ,
                eng:content-descriptor-ideas-power-industry-empire-1745-1901 ,
                eng:content-descriptor-challenges-britain-europe-world-1901-present ,
                eng:content-descriptor-aspect-british-history-before-1066 ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-significant-aspects-of-world-history
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "Significant aspects of the history of the wider world"@en ;
  skos:broader eng:strand-knowledge-of-past ;
  skos:narrower eng:content-descriptor-earliest-civilizations ,
                eng:content-descriptor-ancient-greece ,
                eng:content-descriptor-non-european-society ,
                eng:content-descriptor-significant-society-or-issue ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Content Descriptors
# ----------------------------------------------------------------------

eng:content-descriptor-narratives-analyses-accounts
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "How to construct causal narratives, analyses, arguments and accounts."@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-consequence
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "How to construct narratives, analyses and accounts that focus on consequence."@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-change-and-continuity
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """How to construct narratives, analyses and accounts that focus on change and 
  continuity."""@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-similarity-and-difference
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """How to construct narratives, analyses and accounts that focus on similarity and 
  difference."""@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-historical-significance
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """How to construct narratives, analyses and accounts that focus on how historical 
  significance."""@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-sources-and-evidence
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """How to construct narratives, analyses and accounts that focus on the ways in 
  which historians interact with sources and evidence."""@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-narratives-analyses-accounts-historical-interpretations
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """How to construct narratives, analyses and accounts that focus on the 
  construction of historical interpretations."""@en ;
  skos:broader eng:substrand-second-order-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-historians-investigate-the-past
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About how historians investigate the past."@en ;
  skos:broader eng:substrand-nature-of-historical-enquiry ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-historians-construct-arguments
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About how historians construct historical claims, arguments and accounts."@en ;
  skos:broader eng:substrand-nature-of-historical-enquiry ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-abstract-terms-historically-grounded
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About abstract terms in a historically grounded manner."@en ;
  skos:broader eng:substrand-substantive-concepts ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-changes-within-living-memory
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About changes within living memory. Where appropriate, these should be used to 
  reveal aspects of change in national life."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-events-beyond-living-memory
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About events beyond living memory that are significant nationally or globally 
  [for example, the Great Fire of London, the first aeroplane flight or events commemorated through 
  festivals or anniversaries]."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-lives-of-significant-individuals
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About the lives of significant individuals in the past who have contributed to 
  national and international achievements. Some should be used to compare aspects of life in 
  different periods."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-significant-historical-events-own-locality
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About significant historical events, people and places in their own 
  locality."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-changes-in-britain-stone-age-to-iron-age
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About changes in Britain from the Stone Age to the Iron Age."@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-roman-empire-impact-on-britain
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About the Roman Empire and its impact on Britain."@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-settlement-by-anglo-saxons-and-scots
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About Britain’s settlement by Anglo-Saxons and Scots."@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-viking-and-anglo-saxon-struggle
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About the Viking and Anglo-Saxon struggle for the Kingdom of England to the 
  time of Edward the Confessor."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-local-history-study
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About a local history study."@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-aspect-british-history-beyond-1066
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About a study of an aspect or theme in British history that extends pupils’ 
  chronological knowledge beyond 1066."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-church-state-society-1066-1509
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About the development of Church, state and society in Medieval Britain, and its 
  connections to the wider world 1066-1509."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-church-state-society-1509-1745
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About the development of Church, state and society in Britain and its 
  connections to the wider world 1509-1745."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-ideas-power-industry-empire-1745-1901
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "About ideas, political power, industry and empire: Britain, 1745-1901."@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-challenges-britain-europe-world-1901-present
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About challenges for Britain, Europe and the wider world 1901 to the present 
  day In addition to studying the Holocaust, this could include:."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-aspect-british-history-before-1066
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About an aspect or theme in British history that consolidates and extends 
  pupils’ chronological knowledge from before 1066."""@en ;
  skos:broader eng:substrand-history-of-diverse-britain ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-earliest-civilizations
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About the achievements of the earliest civilizations – an overview of where and 
  when the first civilizations appeared and a depth study of one of the following: Ancient Sumer; 
  The Indus Valley; Ancient Egypt; The Shang Dynasty of Ancient China."""@en ;
  skos:broader eng:substrand-significant-aspects-of-world-history ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-ancient-greece
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About Ancient Greece – a study of Greek life and achievements and their 
  influence on the western world."""@en ;
  skos:broader eng:substrand-significant-aspects-of-world-history ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-non-european-society
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About a non-European society that provides contrasts with British history - In 
  addition to a study of early Islamic civilization, including a study of Baghdad c. AD 900, 
  schools may choose to teach an additional non-European society from the following; Maya 
  civilization c. AD 900; Benin (West Africa) c. AD 900-1300."""@en ;
  skos:broader eng:substrand-significant-aspects-of-world-history ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-significant-society-or-issue
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """About a significant society or issue in world history and its
interconnections with other world developments."""@en ;
  skos:broader eng:substrand-significant-aspects-of-world-history ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Content Sub-Descriptors
# ----------------------------------------------------------------------

eng:content-subdescriptor-seasons-detail
  a skos:Concept, curric:ContentSubDescriptor ;
  skos:prefLabel "Seasons detail"@en ;
  skos:broader eng:content-descriptor-observe-seasonal-changes ;
  skos:inScheme eng:knowledge-taxonomy .

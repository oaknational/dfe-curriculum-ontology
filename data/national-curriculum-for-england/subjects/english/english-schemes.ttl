# ======================================================================
# National Curriculum for England - English Schemes
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/england/english-schemes>
  rdf:type owl:Ontology ;

  rdfs:label "National Curriculum for England - English Schemes"@en ;
  dc:title "National Curriculum for England - English Schemes"@en ;
  rdfs:comment """English schemes that map content descriptors to each scheme."""@en ;

  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/england/english-schemes/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/england/english-schemes/0.0.9> ;
  
  dcterms:description """This file defines English schemes across all key stages (KS1-4), 
  specifying which content descriptors are taught at each stage."""@en ;
  dc:creator "Department for Education" ;
  dcterms:created "2025-11-24"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;
  
  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> ;
  owl:imports <https://w3id.org/uk/curriculum/england/programme-structure> ;
  owl:imports <https://w3id.org/uk/curriculum/england/english-subject> ;
  owl:imports <https://w3id.org/uk/curriculum/england/english-knowledge-taxonomy> .

# ----------------------------------------------------------------------
# Schemes 
# ----------------------------------------------------------------------

eng:scheme-english-key-stage-2
  a curric:Scheme ;
  rdfs:label "English Key Stage 2"@en ;
  rdfs:comment "English programme of study for key stage 2."@en ;
  curric:isSchemeOf eng:subsubject-english ;
  curric:coversKeyStage eng:key-stage-2 .

# ----------------------------------------------------------------------
# Progressions
# ----------------------------------------------------------------------

eng:progression-science-key-stage-3-seasonal-changes
  a curric:Progression ;
  rdfs:label "Science Key Stage 3 Seasonal Changes"@en ;
  rdfs:comment "Science progression for key stage 3: Seasonal changes."@en ;
  curric:isProgressionOf eng:scheme-english-key-stage-2 ;
  curric:coversContent
    eng:content-descriptor-observe-seasonal-changes .
    
# ----------------------------------------------------------------------
# Subject: English
# ----------------------------------------------------------------------

eng:subject-english
  a curric:Subject ;
  rdfs:label "English"@en ;
  rdfs:comment "English subject in the National Curriculum for England."@en ;
  curric:coversDiscipline eng:discipline-science .

# ----------------------------------------------------------------------
# Sub-Subjects 
# ----------------------------------------------------------------------

eng:subsubject-english
  a curric:SubSubject ;
  rdfs:label "English"@en ;
  rdfs:comment "English sub-subject in the National Curriculum for England."@en ;
  dcterms:description """A high-quality english education provides the foundations for 
  understanding ..."""@en ;
  dcterms:source <https://www.gov.uk/government/publications/national-curriculum-in-england-english-programmes-of-study> ;
  curric:isSubSubjectOf eng:subject-english ;
  curric:coversStrand eng:strand-structure-function-living-organisms ,
                   eng:strand-material-cycles-and-energy ;

  curric:hasAims 
    """English aim 1"""@en ,
    """English aim 2"""@en ,
    """English aim 3"""@en .

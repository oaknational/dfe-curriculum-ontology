@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# ======================================================================
# National Curriculum for England - Science Knowledge Taxonomy
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/england/science-knowledge-taxonomy>
  rdf:type owl:Ontology ;

  rdfs:label "National Curriculum for England - Science Knowledge Taxonomy"@en ;
  dc:title "National Curriculum for England - Science Knowledge Taxonomy"@en ;
  rdfs:comment """Science knowledge taxonomy defining the hierarchical organization of scientific 
  content from strands through to content sub-descriptors."""@en ;

  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/england/science-knowledge-taxonomy/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/england/science-knowledge-taxonomy/0.0.9> ;
  
  dcterms:description """This file defines the Science knowledge taxonomy for the National 
  Curriculum in England, organizing content through strands, sub-strands, content descriptors, 
  and content sub-descriptors."""@en ;
  dc:creator "Department for Education" ;
  dcterms:created "2025-11-24"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;
  
  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;

  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "eng" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/england/" ;

  owl:imports <https://w3id.org/uk/curriculum/core/> ;
  owl:imports <https://w3id.org/uk/curriculum/england/science-subject> .

  # ----------------------------------------------------------------------
# Strands
# ----------------------------------------------------------------------

eng:strand-structure-function-living-organisms
  a skos:Concept, curric:Strand ;
  skos:prefLabel "Structure and function of living organisms"@en ;
  skos:broader eng:subject-science ;
  skos:narrower eng:substrand-cells-organisation ,
                eng:substrand-skeletal-and-muscular-systems ;
  curric:applicableToSubSubject eng:subsubject-science ;
  skos:inScheme eng:knowledge-taxonomy .

eng:strand-material-cycles-and-energy
  a skos:Concept, curric:Strand ;
  skos:prefLabel "Material cycles and energy"@en ;
  skos:broader eng:subject-science ;
  skos:narrower eng:substrand-photosynthesis ,
                eng:substrand-cellular-respiration ,
                eng:substrand-seasonal-changes .

  curric:applicableToSubSubject eng:subsubject-science ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Sub-Strands
# ----------------------------------------------------------------------

eng:substrand-cells-organisation
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "Cells and organisation"@en ;
  skos:broader eng:strand-structure-function-living-organisms ;
  skos:narrower eng:content-descriptor-cells-as-unit-of-living-organism ,
                eng:content-descriptor-functions-of-the-cell-wall ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-skeletal-and-muscular-systems
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "The skeletal and muscular systems"@en ;
  skos:broader eng:strand-structure-function-living-organisms ;
  skos:narrower eng:content-descriptor-biomechanics ,
                eng:content-descriptor-function-of-muscles ;
  skos:inScheme eng:knowledge-taxonomy .

eng:substrand-seasonal-changes
  a skos:Concept, curric:SubStrand ;
  skos:prefLabel "Seasonal changes"@en ;
  skos:broader eng:strand-material-cycles-and-energy ;
  skos:narrower eng:content-descriptor-observe-seasonal-changes ;
  skos:inScheme eng:knowledge-taxonomy .

# ----------------------------------------------------------------------
# Content Descriptors
# ----------------------------------------------------------------------

eng:content-descriptor-cells-as-unit-of-living-organism
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """Cells as the fundamental unit of living organisms, including how to observe, 
  interpret and record cell structure using a light microscope."""@en ;
  skos:broader eng:substrand-cells-organisation ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-functions-of-the-cell-wall
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel """The functions of the cell wall, cell membrane, cytoplasm, nucleus, vacuole,
  mitochondria and chloroplasts."""@en ;
  skos:broader eng:substrand-cells-organisation ;
  skos:inScheme eng:knowledge-taxonomy .

eng:content-descriptor-observe-seasonal-changes
  a skos:Concept, curric:ContentDescriptor ;
  skos:prefLabel "Observe changes across the 4 seasons"@en ;
  skos:broader eng:substrand-seasonal-changes ;
  skos:narrower eng:content-subdescriptor-seasons-detail ;
  skos:inScheme eng:knowledge-taxonomy ;
  skos:related eng:theme-climate-change-and-sustainability .

# ----------------------------------------------------------------------
# Content Sub-Descriptors
# ----------------------------------------------------------------------

eng:content-subdescriptor-seasons-detail
  a skos:Concept, curric:ContentSubDescriptor ;
  skos:prefLabel "Seasons detail"@en ;
  skos:broader eng:content-descriptor-observe-seasonal-changes ;
  skos:inScheme eng:knowledge-taxonomy .

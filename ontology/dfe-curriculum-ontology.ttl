@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .

# ======================================================================
# DfE Curriculum Ontology
# ======================================================================

# ----------------------------------------------------------------------
# Ontology Declaration
# ----------------------------------------------------------------------

<https://w3id.org/uk/curriculum/core/curriculum-ontology>
  rdf:type owl:Ontology ;

  rdfs:label "DfE Curriculum Ontology"@en ;
  dc:title "DfE Curriculum Ontology"@en ;
  rdfs:comment """Core vocabulary for describing curriculum structures, content organization, and 
  educational programmes."""@en ;
  
  owl:versionInfo "0.1.0" ;
  owl:versionIRI <https://w3id.org/uk/curriculum/core/curriculum-ontology/0.1.0> ;
  owl:priorVersion <https://w3id.org/uk/curriculum/core/curriculum-ontology/0.0.9> ;

  bibo:status "DfE Curriculum Ontology" ;
  dcterms:abstract """This ontology defines the foundational classes and properties for
  representing educational curriculum structures."""@en ;
  dcterms:description """This ontology defines the foundational classes and properties for
  representing educational curriculum structures."""@en ;
  dcterms:creator "Department for Education" ;
  dcterms:created "2025-11-19"^^xsd:date ;
  dcterms:modified "2025-12-03"^^xsd:date ;
  dcterms:issued "2025-12-03"^^xsd:date ;

  dcterms:license <http://www.nationalarchives.gov.uk/doc/open-government-licence/version/3/> ;
  dcterms:rights "Crown Copyright"@en ;
  
  rdfs:seeAlso <https://www.gov.uk/government/collections/national-curriculum> ;
  vann:preferredNamespacePrefix "curric" ;
  vann:preferredNamespaceUri "https://w3id.org/uk/curriculum/core/" ;

  owl:imports <http://www.w3.org/2004/02/skos/core> .

# ----------------------------------------------------------------------
# Changelog
# ----------------------------------------------------------------------

curric:version-0.1.0
  dcterms:date "2025-12-03"^^xsd:date ;
  rdfs:comment "Initial release"@en .

curric:version-0.0.9
  dcterms:date "2025-11-19"^^xsd:date ;
  rdfs:comment "Development version"@en .

# ----------------------------------------------------------------------
# Classes: Programme Structure
# (Phase, KeyStage, YearGroup, Subject, SubSubject, Scheme, Progression)
# ----------------------------------------------------------------------

curric:Phase 
  rdf:type owl:Class ;
  rdfs:label "Phase"@en ;
  rdfs:comment "A phase of study in the education system."@en ;
  rdfs:isDefinedBy curric: .
    
curric:KeyStage 
  rdf:type owl:Class ;
  rdfs:label "Key Stage"@en ;
  rdfs:comment "A key stage that structures learning for specific age groups."@en ;
  rdfs:isDefinedBy curric: .

curric:YearGroup 
  rdf:type owl:Class ;
  rdfs:label "Year Group"@en ;
  rdfs:comment "A year group within a key stage."@en ;
  rdfs:isDefinedBy curric: .

curric:Subject
  rdf:type owl:Class ;
  rdfs:label "Subject"@en ;
  rdfs:comment "Subject (e.g. science, history, physical education)."@en ;
  rdfs:isDefinedBy curric: .

curric:SubSubject
  rdf:type owl:Class ;
  rdfs:label "Sub-Subject"@en ;
  rdfs:comment "Sub-subject (e.g. biology, history, physical education)."@en ;
  rdfs:isDefinedBy curric: .

curric:Scheme
  rdf:type owl:Class ;
  rdfs:label "Scheme"@en ;
  rdfs:comment "A scheme defines one or more progressions for a sub-subject and a key stage."@en ;
  rdfs:isDefinedBy curric: .

curric:Progression
  rdf:type owl:Class ;
  rdfs:label "Progression"@en ;
  rdfs:comment """A progression defines curriculum content relating to a single sub-strand, for a 
  scheme."""@en ;
  rdfs:isDefinedBy curric: .

# ----------------------------------------------------------------------
# Classes: Knowledge Structure
# (Discipline, Strand, SubStrand, ContentDescriptor, 
# ContentSubDescriptor, Theme)
# ----------------------------------------------------------------------

curric:Discipline
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Discipline"@en ;
  rdfs:comment "An organisational discipline (e.g. mathematics, english, history)."@en ;
  rdfs:isDefinedBy curric: .

curric:Strand
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Strand"@en ;
  rdfs:comment "An organisational strand within a discipline (e.g. number, algebra)."@en ;
  rdfs:isDefinedBy curric: .

curric:SubStrand
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Sub-Strand"@en ;
  rdfs:comment "A sub-division of a strand (e.g. place value, addition and subtraction)."@en ;
  rdfs:isDefinedBy curric: .

curric:ContentDescriptor
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Content Descriptor"@en ;
  rdfs:comment "A statement of knowledge (a concept, fact or skill) within a sub-strand."@en ;
  rdfs:isDefinedBy curric: .

curric:ContentSubDescriptor
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Content Sub-Descriptor"@en ;
  rdfs:comment "Elaboration or specific detail relating to a content descriptor."@en ;
  rdfs:isDefinedBy curric: .

curric:Theme
  rdf:type owl:Class ;
  rdfs:subClassOf skos:Concept ;
  rdfs:label "Theme"@en ;
  rdfs:comment "A cross-cutting theme that can span multiple subjects and content areas."@en ;
  rdfs:isDefinedBy curric: .

# ----------------------------------------------------------------------
# Object Properties
# (hasKeyStage, isKeyStageOf, hasYearGroup, isYearGroupOf, 
# coversKeyStage, isCoveredByScheme, hasDiscipline, isDisciplineOf, coversStrand, 
# isStrandOf, includesContentDescriptor, isIncludedByProgression, hasScheme,
# isSchemeOf, )
# ----------------------------------------------------------------------

curric:hasKeyStage
  rdf:type owl:ObjectProperty ;
  rdfs:label "has key stage"@en ;
  rdfs:comment "Indicates that a phase contains a key stage as a subdivision."@en ;
  rdfs:domain curric:Phase ;
  rdfs:range curric:KeyStage ;
  owl:inverseOf curric:isKeyStageOf ;
  rdfs:isDefinedBy curric: .

curric:isKeyStageOf
  rdf:type owl:ObjectProperty ;
  rdfs:label "is key stage of"@en ;
  rdfs:comment "Indicates that a key stage is a subdivision of a phase."@en ;
  rdfs:domain curric:KeyStage ;
  rdfs:range curric:Phase ;
  owl:inverseOf curric:hasKeyStage ;
  rdfs:isDefinedBy curric: .

curric:hasYearGroup
  rdf:type owl:ObjectProperty ;
  rdfs:label "has year group"@en ;
  rdfs:comment "Indicates that a key stage contains a year group as a subdivision."@en ;
  rdfs:domain curric:KeyStage ;
  rdfs:range curric:YearGroup ;
  owl:inverseOf curric:isYearGroupOf ;
  rdfs:isDefinedBy curric: .

curric:isYearGroupOf
  rdf:type owl:ObjectProperty ;
  rdfs:label "is key stage of"@en ;
  rdfs:comment "Indicates that a year group is a subdivision of a key stage."@en ;
  rdfs:domain curric:YearGroup ;
  rdfs:range curric:KeyStage ;
  owl:inverseOf curric:hasYearGroup ;
  rdfs:isDefinedBy curric: .

curric:coversKeyStage
  rdf:type owl:ObjectProperty ;
  rdfs:label "covers key stage"@en ;
  rdfs:comment "Indicates that a scheme covers a specific key stage."@en ;
  rdfs:domain curric:Scheme ;
  rdfs:range curric:KeyStage ;
  owl:inverseOf curric:isCoveredByScheme ;
  rdfs:isDefinedBy curric: .

curric:isCoveredByScheme
  rdf:type owl:ObjectProperty ;
  rdfs:label "is covered by scheme"@en ;
  rdfs:comment "Indicates that a key stage is covered by a specific scheme."@en ;
  rdfs:domain curric:KeyStage ;
  rdfs:range curric:Scheme ;
  owl:inverseOf curric:coversKeyStage ;
  rdfs:isDefinedBy curric: .

curric:coversDiscipline
  rdf:type owl:ObjectProperty ;
  rdfs:label "covers discipline"@en ;
  rdfs:comment "Indicates that a subject covers content from a discipline."@en ;
  rdfs:domain curric:Subject ;
  rdfs:range curric:Discipline ;
  owl:inverseOf curric:isCoveredBySubject ;
  rdfs:isDefinedBy curric: .

curric:isCoveredBySubject
  rdf:type owl:ObjectProperty ;
  rdfs:label "is covered by subject"@en ;
  rdfs:comment "Indicates that content from a discipline is covered by a subject."@en ;
  rdfs:domain curric:Discipline ;
  rdfs:range curric:Subject ;
  owl:inverseOf curric:coversDiscipline ;
  rdfs:isDefinedBy curric: .

curric:coversStrand
  rdf:type owl:ObjectProperty ;
  rdfs:label "covers strand"@en ;
  rdfs:comment "Indicates that a sub-subject covers content from a strand of knowledge."@en ;
  rdfs:domain curric:SubSubject ;
  rdfs:range curric:Strand ;
  owl:inverseOf curric:isCoveredBySubSubject ;
  rdfs:isDefinedBy curric: .

curric:isCoveredBySubSubject
  rdf:type owl:ObjectProperty ;
  rdfs:label "is covered by sub-subject"@en ;
  rdfs:comment "Indicates that content from a strand of knowledge is covered by a sub-subject."@en ;
  rdfs:domain curric:Strand ;
  rdfs:range curric:SubSubject ;
  owl:inverseOf curric:coversStrand ;
  rdfs:isDefinedBy curric: .

curric:includesContentDescriptor
  rdf:type owl:ObjectProperty ;
  rdfs:label "includes content descriptor"@en ;
  rdfs:comment "Indicates that a progression includes a specific content descriptor."@en ;
  rdfs:domain curric:Progression ;
  rdfs:range curric:ContentDescriptor ;
  owl:inverseOf curric:isIncludedByProgression ;
  rdfs:isDefinedBy curric: .

curric:isIncludedByProgression
  rdf:type owl:ObjectProperty ;
  rdfs:label "is included by progression"@en ;
  rdfs:comment "Indicates that a content descriptor is included in a specific progression."@en ;
  rdfs:domain curric:ContentDescriptor ;
  rdfs:range curric:Progression ;
  owl:inverseOf curric:includesContentDescriptor ;
  rdfs:isDefinedBy curric: .

curric:hasSubSubject
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "has sub-subject"@en ;
  rdfs:comment "Indicates that a subject contains a sub-subject."@en ;
  rdfs:domain curric:Subject ;
  rdfs:range curric:SubSubject ;
  owl:inverseOf curric:isSubSubjectOf ;
  rdfs:isDefinedBy curric: .

curric:isSubSubjectOf
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "is sub-subject of"@en ;
  rdfs:comment "Indicates that a sub-subject is part of a subject."@en ;
  rdfs:domain curric:SubSubject ;
  rdfs:range curric:Subject ;
  owl:inverseOf curric:hasSubSubject ;
  rdfs:isDefinedBy curric: .

curric:hasScheme
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "has scheme"@en ;
  rdfs:comment "Indicates that a sub-subject contains a scheme."@en ;
  rdfs:domain curric:SubSubject ;
  rdfs:range curric:Scheme ;
  owl:inverseOf curric:isSchemeOf ;
  rdfs:isDefinedBy curric: .

curric:isSchemeOf
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "is scheme of"@en ;
  rdfs:comment "Indicates that a scheme is part of a sub-subject."@en ;
  rdfs:domain curric:Scheme ;
  rdfs:range curric:SubSubject ;
  owl:inverseOf curric:hasScheme ;
  rdfs:isDefinedBy curric: .

curric:hasProgression
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "has progression"@en ;
  rdfs:comment "Indicates that a scheme contains a progression."@en ;
  rdfs:domain curric:Scheme ;
  rdfs:range curric:Progression ;
  owl:inverseOf curric:isProgressionOf ;
  rdfs:isDefinedBy curric: .

curric:isProgressionOf
  rdf:type owl:ObjectProperty , owl:TransitiveProperty ;
  rdfs:label "is progression of"@en ;
  rdfs:comment "Indicates that a progression is part of a scheme."@en ;
  rdfs:domain curric:Progression ;
  rdfs:range curric:Scheme ;
  owl:inverseOf curric:hasProgression ;
  rdfs:isDefinedBy curric: .

# ----------------------------------------------------------------------
# Datatype Properties
# (lowerAgeBoundary, upperAgeBoundary, hasAim, example, exampleURL)
# ----------------------------------------------------------------------

curric:lowerAgeBoundary
  rdf:type owl:DatatypeProperty ;
  rdfs:label "lower age boundary"@en ;
  rdfs:comment "Lower age boundary of an educational level."@en ;
  rdfs:domain [ owl:unionOf (curric:Phase curric:KeyStage curric:YearGroup) ] ;
  rdfs:range xsd:nonNegativeInteger ;
  rdfs:isDefinedBy curric: .

curric:upperAgeBoundary
  rdf:type owl:DatatypeProperty ;
  rdfs:label "upper age boundary"@en ;
  rdfs:comment "Upper age boundary of an educational level."@en ;
  rdfs:domain [ owl:unionOf (curric:Phase curric:KeyStage curric:YearGroup) ] ;
  rdfs:range xsd:positiveInteger ;
  rdfs:isDefinedBy curric: .

curric:hasAim
  rdf:type owl:DatatypeProperty ;
  rdfs:label "has aim"@en ;
  rdfs:comment "A specific educational aim for this subject as stated in the National Curriculum."@en ;
  rdfs:domain curric:SubSubject ;
  rdfs:range rdf:langString ;
  rdfs:isDefinedBy curric: .

curric:example
  rdf:type owl:DatatypeProperty ;
  rdfs:label "example"@en ;
  rdfs:comment "A text example that illustrates content detail."@en ;
  rdfs:domain curric:ContentSubDescriptor ;
  rdfs:range rdf:langString ;
  rdfs:isDefinedBy curric: .

curric:exampleURL
  rdf:type owl:DatatypeProperty ;
  rdfs:label "example URL"@en ;
  rdfs:comment "A URL example that illustrates content detail."@en ;
  rdfs:domain curric:ContentSubDescriptor ;
  rdfs:range xsd:anyURI ;
  rdfs:isDefinedBy curric: .
